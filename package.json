{
  "name": "obsidian-gdocs-exporter",
  "version": "1.0.0",
  "description": "Export Obsidian notes to Google Docs-compatible formats with Mermaid diagram support",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "verify": "node scripts/verify-build.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "google-docs",
    "export",
    "mermaid"
  ],
  "author": "KR",
  "license": "MIT",
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/pako": "^2.0.3",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.8",
    "jest": "^30.2.0",
    "obsidian": "latest",
    "ts-jest": "^29.4.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "docx": "^8.5.0",
    "file-saver": "^2.0.5",
    "marked": "^11.1.0",
    "pako": "^2.1.0"
  }
}
